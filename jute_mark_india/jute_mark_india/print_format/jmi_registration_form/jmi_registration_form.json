div class=\"row section-break\">\n\t<div class=\"col-xs-6 column-break\">\n\t\t<div class=\"row\">\n\t\t\t<div class=\"col-xs-5\">\n\t\t\t\t<label>State: </label>\n\t\t\t</div>\n\t\t\t<div class=\"col-xs-7 text-right value\">\n\t\t\t\t{{ doc.state or ''}}\n\t\t\t</div>\n\t\t</div>\n\t\t<div class=\"row\">\n\t\t\t<div class=\"col-xs-5\">\n\t\t\t\t<label>State Code: </label>\n\t\t\t</div>\n\t\t\t<div class=\"col-xs-7 text-right value\">\n\t\t\t\t{{ doc.state_code or ''}}\n\t\t\t</div>\n\t\t</div>\n\t\t<div class=\"row\">\n\t\t\t<div class=\"col-xs-5\">\n\t\t\t\t<label>District: </label>\n\t\t\t</div>\n\t\t\t<div class=\"col-xs-7 text-right value\">\n\t\t\t\t{{ doc.district or ''}}\n\t\t\t</div>\n\t\t</div>\n\t</div>\n\t<div class=\"col-xs-6 column-break\">\n\t\t<div class=\"row\">\n\t\t\t<div class=\"col-xs-5\">\n\t\t\t\t<label>Address Line 1: </label>\n\t\t\t</div>\n\t\t\t<div class=\"col-xs-7 text-right value\">\n\t\t\t\t{{ doc.address_line_1 or ''}}\n\t\t\t</div>\n\t\t</div>\n\t\t<div class=\"row\">\n\t\t\t<div class=\"col-xs-5\">\n\t\t\t\t<label>Pin Code: </label>\n\t\t\t</div>\n\t\t\t<div class=\"col-xs-7 text-right value\">\n\t\t\t\t{{ doc.pin_code or ''}}\n\t\t\t</div>\n\t\t</div>\n\t</div>\n</div>\n<hr>\n<h5>Product Details</h5>\n<table class=\"table table-bordered\">\n\t<tbody>\n\t\t<tr>\n\t\t\t<th>No.</th>\t\n\t\t\t<th>Product Type</th>\n\t\t\t<th>Description</th>\n\t\t\t<th>Fiber Content</th>\n\t\t\t<th>Functional Use Of Jute</th>\n\t\t\t<th>Jute Visible</th>\n\t\t\t<th>TR Detail</th>\n\t\t</tr>\n\t\t{%- for row in doc.textile_details_of_product -%}\n\t\t<tr>\n\t\t\t<td>{{ row.idx }}</td>\n\t\t\t<td>{{ row.product_type or ''}}</td>\n\t\t\t<td>{{ row.product_description or ''}}</td>\n\t\t\t<td>{{ row.fiber_content or ''}}</td>\n\t\t\t<td>{{ row.function_of_jute_material_in_the_product or ''}}</td>\n\t\t\t<td>{{ row.whether_jute_is_visible_or_not_visible_in_the_product or ''}}</td>\n\t\t\t<td>{{ row.test_report_details or ''}}</td>\n\t\t</tr>\n\t\t{%- endfor 